{
  "swagger": "2.0",
  "info": {
    "title": "FootBet",
    "description": "An API to manipulate soccer stats",
    "version": "1.0.0"
  },
  "host": "foot-bet-backend-lesson.onrender.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "paths": {
    "/login": {
      "post": {
        "description": "It returns a token so the user is able to use the other routes",
        "parameters": [
          {
            "in": "body",
            "name": "User Data",
            "description": "User name and password",
            "required": true,
            "schema": {
              "properties": {
                "user": {
                  "type": "string",
                  "example": "anyUser123"
                },
                "password": {
                  "type": "string",
                  "example": "anyPassword123"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "It will return a token to put into the other requests headers"
          }
        }
      }
    },
    "/players": {
      "get": {
        "description": "It returns all players currently on database",
        "parameters": [
          {
            "in": "header",
            "name": "x-access-token",
            "description": "The token generated in /login endpoint",
            "required": true,
            "schema": {
              "properties": {
                "x-access-token": {
                  "type": "string",
                  "example": "eyJhbaGciOiJIUzI1NddiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiI2MzgyYzfY1MGJjOGYyYWE4fdNGExYTczODIiLCJpYXQiOjEf2Njk1MjAxNDIsImV4cCI6MTY2OTUyMDQ0Mn0.zNM-PkImqnBjfytHeEX1POxze9GGo5_mK_gqS5Y2urs"
                }
              }
            }

          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "description": "It adds a new player to the database",
        "parameters": [
          {
            "in": "body",
            "name": "Player",
            "description": "The player to add",
            "required": "true",
            "schema":
              {
                  "properties": {
                      "name": {
                        "type": "string",
                        "example": "Elias Moreira"
                      },
                      "overall": {
                        "type": "number",
                        "example": 69
                      },
                      "nacionality": {
                        "type": "string",
                        "example": "brazil"
                      },
                      "club_id": {
                        "type": "number",
                        "example": 0
                      },
                      "main_position": {
                        "type": "string",
                        "example": "MC"
                      },
                      "photo": {
                        "type": "string",
                        "example": "data:image/png;base64..."
                      }
                  }
              }
          },
          {
            "in": "header",
            "name": "x-access-token",
            "description": "The token generated in /login endpoint",
            "required": true,
            "schema": {
              "properties": {
                "x-access-token": {
                  "type": "string",
                  "example": "eyJhbaGciOiJIUzI1NddiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiI2MzgyYzfY1MGJjOGYyYWE4fdNGExYTczODIiLCJpYXQiOjEf2Njk1MjAxNDIsImV4cCI6MTY2OTUyMDQ0Mn0.zNM-PkImqnBjfytHeEX1POxze9GGo5_mK_gqS5Y2urs"
                }
              }
            }

          }
        ],
        "responses": {
          "201": {
            "description": "success"
          }
        }
      }
    },
    "/players/{id}": {
      "delete": {
        "description": "It deletes specific id player currently on database",
        "parameters": [{
          "in": "path",
          "name": "id",
          "schema": {
            "type": "string",
            "required": "true",
            "description": "Numeric ID of the player to delete"
          }
        },
        {
          "in": "header",
          "name": "x-access-token",
          "description": "The token generated in /login endpoint",
          "required": true,
          "schema": {
            "properties": {
              "x-access-token": {
                "type": "string",
                "example": "eyJhbaGciOiJIUzI1NddiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiI2MzgyYzfY1MGJjOGYyYWE4fdNGExYTczODIiLCJpYXQiOjEf2Njk1MjAxNDIsImV4cCI6MTY2OTUyMDQ0Mn0.zNM-PkImqnBjfytHeEX1POxze9GGo5_mK_gqS5Y2urs"
              }
            }
          }

        }
        ],
        "responses": {
          "200": {
            "description": "success"
          }
        }
      },
      "put": {
        "description": "It updates a player to the database",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "string",
              "required": "true",
              "description": "Numeric ID of the player to update"
            }
          },
          {
            "in": "body",
            "name": "Player",
            "description": "The new player data",
            "required": "true",
            "schema":
              {
                  "properties": {
                      "name": {
                        "type": "string",
                        "example": "Kylian Mbapp√©"
                      },
                      "overall": {
                        "type": "number",
                        "example": 91
                      },
                      "nacionality": {
                        "type": "string",
                        "example": "france"
                      },
                      "club_id": {
                        "type": "number",
                        "example": 1
                      },
                      "main_position": {
                        "type": "string",
                        "example": "ST"
                      },
                      "photo": {
                        "required": false,
                        "type": "string",
                        "example": "data:image/png;base64,"
                      }
                  }
              }
          },
          {
            "in": "header",
            "name": "x-access-token",
            "description": "The token generated in /login endpoint",
            "required": true,
            "schema": {
              "properties": {
                "x-access-token": {
                  "type": "string",
                  "example": "eyJhbaGciOiJIUzI1NddiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiI2MzgyYzfY1MGJjOGYyYWE4fdNGExYTczODIiLCJpYXQiOjEf2Njk1MjAxNDIsImV4cCI6MTY2OTUyMDQ0Mn0.zNM-PkImqnBjfytHeEX1POxze9GGo5_mK_gqS5Y2urs"
                }
              }
            }

          }
        ],
        "responses": {
          "201": {
            "description": "success"
          }
        }
      }
    }
  }
}